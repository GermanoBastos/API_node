{
    "name": "API_node",
    "version": 2,
    "public": true,
    "build": {
      "src": "index.html",
      "use": "@vercel/node"
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.html"
      }
    ],
    "headers": [
      {
        "source": "/(.*)",
        "headers": [
          {
            "key": "Access-Control-Allow-Origin",
            "value": "*"
          }
        ]
      }
    ]
  }